{
  "name": "Ploom",
  "version": "0.0.1",
  "description": "JT／Ploomサイトリニューアル(PJ2012_008)",
  "repository": "https://tci-dm.backlog.jp/git/PJ2012_008/template_component.git",
  "main": "index.js",
  "scripts": {
    "start": "webpack-cli serve --mode development --config scripts/webpack.config.dev.js",
    "build": "node scripts/build.js",
    "scss:lint": "stylelint src/**/*.scss --config .stylelintrc.yml",
    "scss:lint:fix": "stylelint src/**/*.scss --config .stylelintrc.yml --fix",
    "js:lint": "eslint 'src/**/*.js' 'src-babels/**/*.{js,jsx}'",
    "js:lint:fix": "eslint 'src/**/*.js' 'src-babels/**/*.{js,jsx}' --fix",
    "format:js": "prettier --write 'src/**/*.{js,jsx}' 'src-babels/**/*.{js,jsx}'",
    "format:scss": "prettier --write 'src/**/*.scss'",
    "format:scss:debug": "prettier 'src/**/*.scss' --debug-check",
    "format:js:debug": "prettier 'src/**/*.{js,jsx}' 'src-babels/**/*.{js,jsx}' --debug-check",
    "precommit": "npm test",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format:js",
      "eslint"
    ],
    "*.{scss}": [
      "npm run format:scss",
      "npm run scss:lint:fix"
    ]
  },
  "keywords": [
    "web",
    "scss",
    "ES2020"
  ],
  "engines": {
    "node": ">= 14.15.4",
    "npm": ">= 6.14.10"
  },
  "author": "@taikiken",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "autoprefixer": "^10.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "core-js": "^3.8.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "globule": "^1.3.2",
    "html-loader": "^1.3.2",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "img-loader": "^3.0.2",
    "mini-css-extract-plugin": "^1.3.3",
    "node-ssi": "^0.3.2",
    "object.assign": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-scss": "^3.0.4",
    "promise-polyfill": "^8.2.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "slick-carousel": "^1.8.1",
    "terser-webpack-plugin": "^5.1.1",
    "three": "^0.124.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-middleware": "^4.0.3",
    "webpack-dev-server": "^3.11.1",
    "webpack-fix-style-only-entries": "^0.6.0",
    "webpack-hot-middleware": "^2.25.0",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.0.0-beta.4",
    "husky": "^4.3.8",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "jest": "^26.6.3",
    "jquery": "^3.5.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "webpack-remove-empty-scripts": "^0.7.0"
  }
}
